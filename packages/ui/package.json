{
  "name": "@yaksok/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*.tsx",
    "./styles.css": "./dist/styles.css",
    "./Colors": "./src/Colors.tsx"
  },
  "scripts": {
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "build": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@turbo/gen": "^2.5.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@yaksok/tailwindcss-config": "workspace:*",
    "@yaksok/typescript-config": "workspace:*",
    "@vitest/coverage-istanbul": "^3.0.7",
    "vitest": "^3.1.3",
    "@yaksok/test-config": "workspace:*",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.12",
    "@yaksok/utils": "workspace:*",
    "@yaksok/icons": "workspace:*",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.4"
  }
}
